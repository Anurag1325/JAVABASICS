import java.util.Scanner;
class anurag{
  public static void main(String[] args){
  Scanner sc=new Scanner(System.in);
  System.out.println("enter number:");
  int number=sc.nextInt();
  int i=1;
  
  int factorial=1;
  while(i<number){
      
      factorial+=factorial*i;
      i++;
     
  }
   System.out.println(factorial);
  }
}
  
  
  
  
